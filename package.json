{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "version": "node ./scripts/version.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "expo": "expo",
    "test": "jest",
    "lint": "eslint .&&tsc --noemit",
    "eas:build": "eas build -p android --non-interactive",
    "eas:submit": "eas submit -p android --latest",
    "eas:deploy": "npm run eas:build&&npm run eas:submit",
    "emulator:list": "%userProfile%\\AppData\\Local\\Android\\sdk\\emulator\\emulator -list-avds",
    "emulator:pixel4": "start %userProfile%\\AppData\\Local\\Android\\sdk\\emulator\\emulator -avd Pixel_4_API_30"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@react-native-async-storage/async-storage": "1.23.1",
    "apollo-link-context": "^1.0.20",
    "date-fns": "^2.28.0",
    "expo": "^51.0.20",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-location": "~17.0.1",
    "expo-notifications": "~0.28.9",
    "expo-sharing": "~12.0.1",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.19",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-maps": "1.14.0",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "4.10.1",
    "react-native-svg": "15.2.0",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^7.2.6",
    "react-router-native": "^6.1.1",
    "redux": "^4.1.2",
    "username-generator": "^1.1.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.5.2",
    "@types/react": "~18.2.14",
    "@types/react-dom": "~18.2.25",
    "@types/react-native-table-component": "^1.2.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "metro-babel-register": "^0.73.3",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFiles": [
      "./src/__tests__/mocks/jestSetupFile.js"
    ],
    "testRegex": "(.*\\.spec\\.(tsx|ts|js)$)"
  },
  "private": true
}
