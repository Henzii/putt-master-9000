{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "version": "node ./scripts/version.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "expo": "expo",
    "test": "jest",
    "lint": "eslint .&&tsc --noemit",
    "eas:build": "eas build -p android --non-interactive",
    "eas:preview": "eas build --profile preview --platform android",
    "eas:submit": "eas submit -p android --latest",
    "eas:deploy": "npm run eas:build&&npm run eas:submit",
    "emulator:list": "%userProfile%\\AppData\\Local\\Android\\sdk\\emulator\\emulator -list-avds",
    "emulator": "start %userProfile%\\AppData\\Local\\Android\\sdk\\emulator\\emulator -avd"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@react-native-async-storage/async-storage": "2.1.2",
    "apollo-link-context": "^1.0.20",
    "color": "^5.0.0",
    "date-fns": "^2.28.0",
    "expo": "~53.0.0",
    "expo-av": "~15.1.7",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.12.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-error-boundary": "^1.2.4",
    "react-native-maps": "1.20.1",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-svg": "15.11.2",
    "react-native-view-shot": "4.0.3",
    "react-redux": "^9.2.0",
    "react-router-native": "^6.1.1",
    "redux": "^5.0.0",
    "username-generator": "^1.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.0",
    "@types/react-native-table-component": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "metro-babel-register": "^0.73.3",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|color)"
    ],
    "setupFiles": [
      "./src/__tests__/mocks/jestSetupFile.js"
    ],
    "testRegex": "(.*\\.spec\\.(tsx|ts|js)$)"
  },
  "private": true
}
